{
    "worldmodel_type": "worldmodel_v1",
    "worldmodel_config": {
      "mdk_config_toml_path": "${APA_MSD_WORLDMODEL_MDK_CONFIG}",
      "mdk_ehr_log_path": "${EHR_WORLDMODEL_LOG_PATH}",
      "enable_ddmap": true, 
      "force_ddmap": true,
      "enable_all_traffic_light": false,
      "task_config": {
        "DdmapGeneratorTask" : {
          "running_thread_id": 0, 
          "running_thread_name": "wm_ddmap_task"
       },
 
        "TimerTask" : {
          "running_thread_id": 0, 
          "running_thread_name": "wm_timer_task",
          "frequency": 10
        },
        "MDKUpdateManagerTask": {
          "running_thread_id": 0,
          "running_thread_name": "wm_mdk_mng"
        },
        "RoutingTask": {
          "running_thread_id": 0,
          "running_thread_name": "wm_routing"
        },
        "LaneRoutingTask": {
          "running_thread_id": 0,
          "running_thread_name": "wm_lane_rout",
          "enable_split_extra_cost": false
        },
        "BoundaryFittingTask": {
          "running_thread_id": 0,
          "running_thread_name": "wm_bdy_fit"
        },
        "LaneGeneratorTask": {
          "running_thread_id": 0,
          "running_thread_name": "wm_lane_gen"
        },
        "InfoGeneratorTask": {
          "running_thread_id": 0,
          "running_thread_name": "wm_info_gen"
        },
        "MapMessageSyncTask": {
          "running_thread_id": 0,
          "running_thread_name": "wm_map_sync"
        },

        "MapFusionTask": {
          "running_thread_id": 0,
          "running_thread_name": "wm_map_fusion"
        },

        "MDKUpdaterCounterTask": {
          "running_thread_id": 0,
          "running_thread_name": "wm_mdk_count"
        },
        "EHRTask": {
          "running_thread_id": 0 ,
          "running_thread_name": "wm_ehr"
        },

        "FusionTask": {
          "running_thread_id": 7,
          "running_thread_name": "wm_fusion",
          "enable_fusion_filter": true,
          "enable_fusion_cone_filter": false,
          "enable_force_filter": true,
          "force_filter_x_distance": 165.0
        }
      },

      "node_status": {
        "frequency": 10,
        "thread_name": "wm_monitor",
        "input": [
          {
            "topic_name": "/mla/egopose",
            "enable": true,
            "normal_hz": 50,
            "warning_hz": 30, 
            "error_hz": 10,
            "hdmap": true,
            "ddmap": true 
 
          },
          {
            "topic_name": "/perception/fusion/object",
            "enable": true,
            "normal_hz": 10,
            "warning_hz": 7, 
            "error_hz": 5,
            "hdmap": false,
            "ddmap": true 
          },
          {
            "topic_name": "/perception/vision/lane",
            "enable": true,
            "normal_hz": 10,
            "warning_hz": 7,
            "error_hz": 5,
            "hdmap": false,
            "ddmap": true 
 
           },
           {
            "topic_name": "/perception/vision/landmark",
            "enable": true,
            "normal_hz": 10,
            "warning_hz": 7,
            "error_hz": 5,
            "hdmap": false,
            "ddmap": true 
 
           },
           {
            "topic_name": "/hdmap/horizon",
            "enable": true,
            "normal_hz": 10,
            "warning_hz": 7,
            "error_hz": 5,
            "hdmap": true,
            "ddmap": false
            }
        ],
        "output":[ 
         {
            "topic_name": "/worldmodel/processed_map",
            "enable": true,
            "normal_hz": 10,
            "warning_hz": 8,
            "error_hz": 5,
            "hdmap": true,
            "ddmap": true 
          },
          {

            "topic_name": "/worldmodel/objects",
            "enable": true,
            "normal_hz": 10,
            "warning_hz": 8,
            "error_hz": 5,
            "hdmap": true,
            "ddmap": true 
          },
          {
            "topic_name": "/worldmodel/info",
            "enable": true,
            "normal_hz": 10,
            "warning_hz": 8,
            "error_hz": 5,
            "hdmap": true,
            "ddmap": false 
          },
          {
            "topic_name": "/worldmodel/map_matched_info",
            "enable": true,
            "normal_hz": 10,
            "warning_hz": 8,
            "error_hz": 5,
            "hdmap": true,
            "ddmap": false
          }
        ]
      }
    },

    "logger": {
      "level": "error",
      "file": "${LOG_CONFIG_DIR}/worldmodel.log",
      "file_type": "write",
      "max_file_size": 5242880,
      "max_file_num": 2,
      "max_buffer_size_KB": 2,
      "enable_stderr": false,
      "enable_background_dump": true,
      "export_frequence": 10,
      "rostopic_name": "/mlog/logs/worldmodel",
      "mfrtopic_name": "/mlog/logs/worldmodel",
      "enable_callback": false,
      "topic_type":"mfr",
      "tag": "highway_worldmodel"
    },
    "timer": {
       "timeline_type": "system",
      "sleep_duration" : 200
    }

}
  
